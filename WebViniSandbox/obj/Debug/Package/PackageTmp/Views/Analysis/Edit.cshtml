@model ViniSandbox.Models.analysis

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>analysis</legend>

        @Html.HiddenFor(model => model.id)

        <div class="editor-label">
            @Html.LabelFor(model => model.start_date)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.start_date)
            @Html.ValidationMessageFor(model => model.start_date)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.final_date)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.final_date)
            @Html.ValidationMessageFor(model => model.final_date)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.id_file_detail, "file_detail")
        </div>
        <div class="editor-field">
            @Html.DropDownList("id_file_detail", String.Empty)
            @Html.ValidationMessageFor(model => model.id_file_detail)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.file_name)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.file_name)
            @Html.ValidationMessageFor(model => model.file_name)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
